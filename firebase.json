{
  "hosting": {
   
    "source": "./",
    "target": "javaGalaxy",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/sitemap.xml",
        "destination": "/sitemap.xml"
      },
      {
        "source": "**/*.@(css|js)",
        "destination": "/index.html"
      }
    ]}
}