{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "javaGalaxy": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/javaGalaxy/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/aos/dist/aos.css",
              "src/styles.scss"
            ],
            "scripts": [
              "./node_modules/aos/dist/aos.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1400kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "16kb",
                  "maximumError": "18kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "javaGalaxy:build:production"
            },
            "development": {
              "browserTarget": "javaGalaxy:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "javaGalaxy:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              
              "src/styles.scss"                        
            ],
            "scripts": []
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/javaGalaxy/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json",
            "inlineStyleLanguage": "scss"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "javaGalaxy:build:development",
              "serverTarget": "javaGalaxy:server:development"
            },
            "production": {
              "browserTarget": "javaGalaxy:build:production",
              "serverTarget": "javaGalaxy:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": [
              "/home",       

              "/code/lang/it",    
              "/code/lang/en",   
              
              "/code/Java%20base/chapter/1/lang/it",
              "/code/Java%20base/chapter/1/subchapter/1.1/lang/it",
              "/code/Java%20base/chapter/1/subchapter/1.2/lang/it",
              "/code/Java%20base/chapter/1/subchapter/1.3/lang/it",
              "/code/Java%20base/chapter/1/subchapter/1.4/lang/it",
              "/code/Java%20base/chapter/1/subchapter/1.5/lang/it",
              
              
              "/code/Basic%20Java/chapter/1/lang/en",
              "/code/Basic%20Java/chapter/1/subchapter/1.1/lang/en",
              "/code/Basic%20Java/chapter/1/subchapter/1.2/lang/en",
              "/code/Basic%20Java/chapter/1/subchapter/1.3/lang/en",
              "/code/Basic%20Java/chapter/1/subchapter/1.4/lang/en",
              "/code/Basic%20Java/chapter/1/subchapter/1.5/lang/en"
            ]
          },
          "configurations": {
            "production": {
              "browserTarget": "javaGalaxy:build:production",
              "serverTarget": "javaGalaxy:server:production"
            },
            "development": {
              "browserTarget": "javaGalaxy:build:development",
              "serverTarget": "javaGalaxy:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
